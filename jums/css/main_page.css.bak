.righttitle{
    font-size: 25px;
    color: #1e90ff;
    margin-top: 25px;
    margin-left: 10px;
    margin-bottom: 10px;
    padding-top: 10px;


}

.news_box{
	width: 800px;
	height: auto;	
	color: #1e90ff;
	margin-left: 20px;
	margin-right: 10px;
	margin-top:20px;
	font-size: 18px;
	padding:0px 0px 20px 40px;

}

.news_box ul{
	margin: 0px;
	padding: 0px;
}

.news_box ul li{
	list-style-type: decimal;
	text-align: left;
}

.news_box a:hover{
	text-decoration: underline;
	color: #fff;
}
.news_box a:link,.news_box a:visited{
	text-decoration: none;
	color: #1e90ff;
}
.news_box a:active{
	text-decoration: none;
	color: #1e90ff;
}


.photo_news_area{
	width: 840px;
	height: 110px;
	margin-top:0px;
	margin-left: 5px;
	margin-right: 5px;

	float: left;
}
img{
	width: 100px;
	height: 100px;
	margin:10px 0 10px 10px;
}
