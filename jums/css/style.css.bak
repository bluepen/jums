.loginleft{
  padding:0px;
  margin:0 auto;
  width:580px;
  height: 38px;
  font-size:20px;
  float:left;
  z-index:10;

  }
  .loginleft ul{
    padding:0px;
    margin:0px;

  }
  .loginleft li{
    list-style: none;
    float:left;
    text-align:center;
    width:100px;
    height: 40px;
  }

  .loginleft a{
  display:block;
  color:#1e90ff;
  padding:5px;
  margin:5px 5px 5px 5px;
  }
  .loginleft a:link,.loginleft a:visited{
  color:#ffffff;
  background-color: #1e90ff;
